<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingEnd="@dimen/qb_px_10"
    android:paddingStart="@dimen/qb_px_10"
    android:paddingTop="@dimen/qb_px_5"
    android:paddingBottom="@dimen/qb_px_5"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/game_list_item"
        android:layout_width="0dp"
        android:layout_height="@dimen/qb_px_32"
        android:layout_weight="1"
        android:gravity="center"
        android:text="game_list_item"
        android:background="@drawable/app_list_corner_round"
        android:textColor="@color/myblack"
        android:textSize="18sp"
        tools:ignore="RtlSymmetry" />

</LinearLayout>